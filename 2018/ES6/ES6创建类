
class Shape{
    constructor(w,l){
        this.w = w;
        this.l = l;
    }
    
    render(){
        console.log("开始构建图形...")
    }
}


class Circle extens Shape{
    constructor(w, l, radius){
        super(w, l);
        this.radius = radius;
    }
    
    static getpi(){
        return 3014;
    }
    
    get area(){
        return Circle.pi * this.radius*this.radius;
    }
    
    render(){
        console.log("正在构建圆形...");
    }
}
